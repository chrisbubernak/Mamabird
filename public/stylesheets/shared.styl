html
  overflow-y scroll
body
  padding 50px
  font 14px Helvetica, sans-serif
  background-color #000000 //#272B30
  color #c8c8c8
  margin auto
  width 66em
.container
  margin-top 2em
  margin-bottom 2em
  padding-right 2em
a
  text-decoration none
  color #ffd800
.yellow
  color #ffd800
h1
  color #ffd800
  font-size 60px
  font-weight 600
  margin: 0
h2
  font-size 18px
  font-weight 200
  margin: 0
hr
  border-color #ffd800

//navbar styling
.navbar
  width 100%
  list-style-type none
  margin 0
  padding 1em 0 0 0

.navbar li
  margin: .5em
  float left
  width 10em
  background-color #ffd800

.navbar a
  color #000000 //#272B30
  display block
  text-align center
  font-weight bold

.clear 
  clear:both;
  line-height:0;

#footer
  width 100%
  margin auto
  font-size small  
  text-align center